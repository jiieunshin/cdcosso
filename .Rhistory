# f = 5 * f1(x[,1]) + 4 * ((3 * x[, 2] - 2)^2 - 1) + 6 * f5(x[, 3]) + 1 * f4(x[, 4]) + 1 * f3(x[, 5])
surTime = rexp(n, exp(f))
cenTime = rexp(n, exp(-f) * runif(1, 4, 6))
y = cbind(time = apply(cbind(surTime, cenTime), 1, min), status = 1 * (surTime < cenTime))
