# conv = max(loss) < 1e-12
loss[j] = abs(theta.old[j] - theta.new[j])
conv2 = sum(loss == 0) == d
conv3 = max(loss) > 5
# cat("i = ", i, "j =", j, "theta.new[j] =", theta.new[j], "loss =", max(loss), "\n")
if(conv2 | conv3){
conv = TRUE
} else{
conv = max(loss[loss > 0]) < 1e-18
}
if(conv) break
theta.old[j] = theta.new[j]
}
if(conv) break
}
