measure[k] <- cosso::PartialLik(time, status, RS, Rtheta %*% fit$c.new) + sum(status == 1)/n^2 * (sum(diag(fit$UHU))/(n - 1) - sum(fit$UHU)/(n^2 - n))
# HH =  fit$H - 2 * cand.lambda[k] * Rtheta
# HHH = ginv(HH/cand.lambda[k] + Rtheta)
