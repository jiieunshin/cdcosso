}
Grad.Term[k] = status[k] - Sum.exp.eta.Grad
weight[k] = Sum.exp.eta.Hess
z[k] = eta - ifelse(weight[k] != 0, - Grad.Term[k]/weight[k], 0)
}
return(list(z = z, gradient = Grad.Term, weight = weight))
}
